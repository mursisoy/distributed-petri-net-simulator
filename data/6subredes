			 POST p2;
			t2: PRE p2;
			 POST p3;
			t3: PRE p3;
			 POST p4;
			t4: PRE p4;
			 POST p5;
			t5: PRE p5;
			 POST p6;
			t6: PRE p6;
			 POST p7;
			t7: PRE p7;
			 POST p8;
			t8: PRE p8;
			 POST p9;
		TIEMPO t0,t1,t2,t3,t4,t5,t6,t7,t8;
		INTERFASE 
			ENTRADA p0;
			SALIDA p9;
	FINSUBRED;

