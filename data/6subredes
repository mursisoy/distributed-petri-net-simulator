			 POST p9;
		TIEMPO t0,t1,t2,t3,t4,t5,t6,t7,t8;
		INTERFASE 
			ENTRADA p0;
			SALIDA p9;
	FINSUBRED;

	SUBRED sub4;
		LUGARES p0,p1,p2,p3,p4,p5,p6,p7,p8,p9;
		TRANSICIONES
			t0: PRE p0;
			 POST p1;
