SINCRONIZACION
	(sub0,p1,p2,p3,p4,p5,p6,p7,p8,p9,p10) <=(p1global,p2global,p3global,p4global,p5global,p6global,p7global,p8global,p9global,p10global)
	(sub1,p0,p9) <= (p1global,p6global)
	(sub2,p0,p9) <= (p2global,p7global)
	(sub3,p0,p9) <= (p3global,p8global)
	(sub4,p0,p9) <= (p4global,p9global)
	(sub5,p0,p9) <= (p5global,p10global)
