/*
	Ejemplo con estos parametros
		Nombre fichero: 6subredes
		Numero horizontales: 5
		Numero verticales: 10
		Numero bloques: 8
*/
RED prueba;

	SUBRED sub0;
		LUGARES p0,p1,p2,p3,p4,p5,p6,p7,p8,p9,p10;
		TRANSICIONES
			t0: PRE p0;
			 POST p1,p2,p3,p4,p5;
			t1: PRE p6,p7,p8,p9,p10;
			 POST p0;
		MARCADO
			p0;
		TIEMPO
			t0,t1;
		INTERFASE
			ENTRADA p6,p7,p8,p9,p10;
			SALIDA p1,p2,p3,p4,p5;
	FINSUBRED;

	SUBRED sub1;
		LUGARES p0,p1,p2,p3,p4,p5,p6,p7,p8,p9;
		TRANSICIONES
			t0: PRE p0;
			 POST p1;
